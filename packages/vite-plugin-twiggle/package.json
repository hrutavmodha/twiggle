{
    "name": "vite-plugin-twiggle",
    "version": "1.6.4",
    "description": "The Vite Plugin for Runtime of Twiggle",
    "main": "./dist/index.js",
    "type": "module",
    "keywords": [
        "vite",
        "twiggle-plugin",
        "jsx",
        "transpiler"
    ],
    "author": "Hrutav Modha",
    "maintainers": [
        "Hrutav Modha"
    ],
    "files": [
        "./dist",
        "./types",
        "./package.json",
        "./README.md"
    ],
    "license": "MIT",
    "devDependencies": {
        "esbuild": "^0.25.10",
        "typescript": "^5.9.2"
    },
    "scripts": {
        "build": "esbuild ./src/index --bundle --platform=node --format=esm --outdir=dist/ --external:@rollup/pluginutils --external:twiggle-plugin-core"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./types/index.d.ts"
        }
    },
    "peerDependencies": {
        "vite": "^7.1.8"
    },
    "dependencies": {
        "@rollup/pluginutils": "^5.3.0",
        "twiggle-plugin-core": "^1.6.0"
    }
}
